{
  "name": "data-streaming",
  "type": "module",
  "version": "0.0.1",
  "prettier": {
    "astroAllowShorthand": true,
    "overrides": [
      {
        "files": "*.astro",
        "options": {
          "parser": "astro"
        }
      }
    ],
    "pluginSearchDirs": [
      "."
    ],
    "plugins": [
      "prettier-plugin-astro"
    ]
  },
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/node": "^6.0.0",
    "@astrojs/solid-js": "^3.0.1",
    "@astrojs/tailwind": "^5.0.0",
    "astro": "^3.1.0",
    "solid-js": "^1.7.12",
    "tailwindcss": "^3.3.3"
  },
  "devDependencies": {
    "prettier-plugin-astro": "^0.12.0"
  }
}
